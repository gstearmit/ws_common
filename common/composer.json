{
    "name": "gstearmit/common",
    "type": "library",
    "description": "Common components for weshop product processing library",
    "keywords": [
        "gateway",
        "ebay",
        "amazon",
        "product"
    ],
    "homepage": "https://github.com/gstearmit/ws_common.git",
    "license": "MIT",
    "authors": [
        {
            "name": "gstearmit",
            "email": "gstearmit@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "WSProduct\\Common\\" : "src/" },
        "classmap": ["src/WSProduct/Omnipay.php"]
    },
    "require": {
        "php": ">=5.3.2",
        "guzzle/guzzle": "~3.9",
        "symfony/http-foundation": "~2.1|~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5.x-dev"
        },
        "gateways": [
            "EBAY",
            "AMAZON"
        ]
    }
}
